{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run ConsoleApp1 Pipeline",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/LocalAgent/LocalAgent.csproj",
        "--",
        "ConsoleApp1",
        "pipeline.yml",
        "--work",
        "work",
        "--id",
        "1"
      ],
      "cwd": "${workspaceFolder}/Samples",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "env": {
        "TeamProject": "TP"
      }
    },
    {
